{
  "builds": [
    {
      "src": "src/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "src/main.py"
    },
    {
      "src": "src/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      },
      "continue": true
    }
  ]
}